{
  "name": "cccbundlezip",
  "version": "2.0.7",
  "description": " cocos creator bundle解压zip",
  "main": "./dist/cjs/index.min.js",
  "module": "./dist/esm/index.min.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.min.js",
      "import": "./dist/cjs/index.min.js"
    },
    "./dev": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/cjs/index.js"
    },
    "./esm": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.min.js"
    },
    "./esm/dev": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types && npm run minify",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "minify": "npm run minify:cjs && npm run minify:esm",
    "minify:cjs": "esbuild src/index.ts --bundle --outfile=dist/cjs/index.min.js --minify --format=cjs --platform=node --keep-names --external:canvas --external:text-encoding --external:xmlhttprequest && rm dist/cjs/index.js",
    "minify:esm": "esbuild src/index.ts --bundle --outfile=dist/esm/index.min.js --minify --format=esm --keep-names --external:canvas --external:text-encoding --external:xmlhttprequest && rm dist/esm/index.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haizeiym/cccbundlezip.git"
  },
  "keywords": [
    "cocoscreator",
    "zip",
    "bundle"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.25.11",
    "terser": "^5.44.0",
    "typescript": "^4.9.5"
  },
  "bugs": {
    "url": "https://github.com/haizeiym/cccbundlezip/issues"
  },
  "homepage": "https://github.com/haizeiym/cccbundlezip#readme",
  "dependencies": {
    "canvas": "^3.2.0",
    "rimraf": "^6.0.1",
    "text-encoding": "^0.7.0",
    "xmlhttprequest": "^1.8.0"
  }
}
